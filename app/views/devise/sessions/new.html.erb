<section id="signin" class="py-3">


<div class="container">
<div class="row">

<div class="col-md-4">

</div>

<div class="col-md-4">

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<form class="form-signin">


 <h1 class="h3 mb-3 font-weight-normal text-dark">Please sign in</h1>
  <label for="inputEmail text-dark" class="sr-only text-dark">Email address</label>

  <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { class: "form-control" }, wrapper: false, label_html: { class: "label text-dark" } %>

    <%= f.input :password,
                required: false,
                input_html: { class: "form-control" }, wrapper: false, label_html: { class: "label text-dark" } %>
<%#= f.input :remember_me, require: true, input_html: { class: "form-control" }, wrapper: false, label_html: { class: "label text-white" }, as: :boolean if devise_mapping.rememberable? %>

<%= f.input :remember_me,require: true, as: :boolean if devise_mapping.rememberable? %>

    <div class="checkbox mb-3">
    <label>
    
    </label>
  </div>


   <%= f.button :submit, "Log in", class: "btn btn-lg btn-primary btn-block" %>
</form>

   
<% end %>

<%= render "devise/shared/links" %>

  </div>

<div class="col-md-4">

</div>

</div>

</div>

</section>


